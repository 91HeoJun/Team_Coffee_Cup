<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.mapper.StoreMapper">

	<!-- 새로운 매장 등록하기 -->
	<insert id="insert" >
		insert into storeTBL(code, name, address, pic) values(sq_store.nextval, #{name}, #{address}, #{pic})
	</insert>
	
	<!-- 등록 매장 불러오기 -->
	<select id="selectAll" resultType="com.company.domain.StoreVO">
		select * from storeTBL
	</select>

</mapper>